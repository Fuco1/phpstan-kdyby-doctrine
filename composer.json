{
    "name": "phpstan/phpstan-kdyby-doctrine",
    "description": "Kdyby/Doctrine extensions for PHPStan",
    "license": ["MIT"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        }
    },
    "require": {
        "php": "~7.0",
        "phpstan/phpstan": "^0.7 || ^0.8",
        "kdyby/doctrine": "~3.1"
    },
    "require-dev": {
        "consistence/coding-standard": "~0.13.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "slevomat/coding-standard": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "PHPStan\\": "src/",
            "Tests\\": "tests/"
        }
    }
}
